#!/usr/local/bin/stsh
context loadFramework:'MPWSideWeb'
server := MPWSchemeHttpServer new.
server server setPort:8081.
base := ref:file:{env:HOME} asScheme.
cache := base cachedBy: MPWSiteMap scheme.

server setScheme: cache .
#server setDelegate: base.

server start:nil.
shell runInteractiveLoop

